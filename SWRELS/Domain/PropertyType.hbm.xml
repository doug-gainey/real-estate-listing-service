<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="SWRELS.Domain.PropertyType, SWRELS" table="PropType">
		<id name="Id" type="Int32">
			<column name="PropType_ID" sql-type="int" not-null="true" unique="true" index="PK_PropType"/>
			<generator class="native" />
		</id>

		<!-- Properties -->
    <property name="ListingType" type="Int32" column="ListType_ID"/>
    <property name="Description" type="String" column="PropertyType"/>
    <property name="PTypeName" type="String"/>
  </class>
</hibernate-mapping>
